<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="CustomRatingBar" parent="@style/Widget.AppCompat.RatingBar">
        <item name="android:progressDrawable">@drawable/custom_ratingbar</item>
        <!--<item name="android:indeterminateDrawable">@drawable/custom_ratingbar</item>-->
        <item name="android:minHeight">22dip</item>
        <item name="android:maxHeight">22dip</item>
        <item name="android:isIndicator">false</item>
    </style>

    <style name="rtBar" parent="@style/Widget.AppCompat.RatingBar.Indicator">
        <item name="android:isIndicator">false</item>

    </style>

</resources>
